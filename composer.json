{
    "name": "ongr/filter-manager-bundle",
    "description": "Filter manager bundle for product lists",
    "type": "symfony-bundle",
    "homepage": "http://ongr.io",
    "version": "6.2",
    "license": "MIT",
    "authors": [
        {
            "name": "ONGR team",
            "homepage": "http://www.ongr.io"
        }
    ],
    "require": {
        "php": "^7.4",
        "symfony/symfony": "^5.4.4",
        "ongr/elasticsearch-bundle": "^6.2",
        "jms/serializer-bundle": "^3.10"
    },
    "require-dev": {
        "monolog/monolog": "^2.35",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "~2.4",
        "satooshi/php-coveralls": "~1.0"
    },
    "autoload": {
        "psr-4": { "ONGR\\FilterManagerBundle\\": "" },
        "exclude-from-classmap": ["/Test/", "/Tests/"]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
